{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "distribution": "store",
        "credentialsSource": "remote"
      },
      "android": {
        "distribution": "store"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_MALSAMI_SERVER_BASEURL": "$EXPO_PUBLIC_MALSAMI_SERVER_BASEURL"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6749496318",
        "appleId": "kjh002899@gmail.com"
      },
      "android": {
        "serviceAccountKeyPath": "./service-account.json",
        "track": "internal"
      }
    }
  }
}
